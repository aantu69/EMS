<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <meta http-equiv="X-UA-Compatible" content="IE=Edge">
        <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
        <title>@ViewBag.Title - EMS</title>
        <!-- Favicon-->
        <link rel="icon" href="favicon.ico" type="image/x-icon">

        <!-- Bootstrap 3.3.7 -->
        @Styles.Render("~/bootstrap/css")
        @Styles.Render("~/jquery-ui/css")
        <!-- Font Awesome -->
        @Styles.Render("~/font-awesome/css")
        <!-- Ionicons -->
        @Styles.Render("~/ionicons/css")
        <!-- iCheck -->
        @Styles.Render("~/iCheck/css")
        <!--select2 -->
        @Styles.Render("~/select2/css")
        <!-- Theme style -->
        @Styles.Render("~/theme/css")
        <!-- AdminLTE Skins. -->
        @Styles.Render("~/adminSkin/css")
        <!-- datatable -->
        @Styles.Render("~/datatable/css")
        <!-- custom -->
        @Styles.Render("~/custom/css")




        <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
        <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
        <![endif]-->
        <!-- Google Fonts -->
        <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,600,700,300italic,400italic,600italic" rel="stylesheet" type="text/css">
        <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet" type="text/css">
        @Scripts.Render("~/bundles/modernizr")

    </head>

    <body class="hold-transition skin-red sidebar-mini" ng-app="RootApp">
        <div class="wrapper">

            <header class="main-header">
                <!-- Logo -->
                <a href="index2.html" class="logo">
                    <!-- mini logo for sidebar mini 50x50 pixels -->
                    <span class="logo-mini"><b>EMS</b></span>
                    <!-- logo for regular state and mobile devices -->
                    <span class="logo-lg"><b>Admin</b> EMS</span>
                </a>
                <!-- Header Navbar: style can be found in header.less -->
                <nav class="navbar navbar-static-top">
                    <!-- Sidebar toggle button-->
                    <a href="#" class="sidebar-toggle" data-toggle="push-menu" role="button">
                        <span class="sr-only">Toggle navigation</span>
                    </a>

                    <div class="navbar-custom-menu">
                        @Html.Partial("_LoginPartial")
                    </div>
                </nav>
            </header>
            <!-- Left side column. contains the logo and sidebar -->
            <aside class="main-sidebar">
                @*@Html.Partial("_MenuPartial")*@
                @{Html.RenderAction("Index", "SuperAdminSideMenu");}
            </aside>

            <!-- Content Wrapper. Contains page content -->
            <div class="content-wrapper">
                <!-- Content Header (Page header) -->
                <section class="content-header">
                    <h1>
                        Dashboard
                        <small>Control panel</small>
                    </h1>
                    <ol class="breadcrumb">
                        <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
                        <li class="active">Dashboard</li>
                    </ol>
                </section>

                <!-- Main content -->
                <section class="content">
                    <div class="row">
                        @RenderBody()
                    </div>
                </section>
                <!-- /.content -->
            </div>
            <!-- /.content-wrapper -->
            <footer class="main-footer">
                <div class="pull-right hidden-xs">
                    <b>Version</b> 2.4.0
                </div>
                <strong>Copyright &copy; 2015-@DateTime.Now.Year <a href="http://softtechlab.com">SoftTech</a>.</strong> All rights
                reserved.
            </footer>
        </div>
        <!-- jQuery 3 -->
        @Scripts.Render("~/bundles/jquery")
        <!-- jQuery-ui -->
        @Scripts.Render("~/bundles/jquery-ui")
        <!-- Bootstrap 3.3.7 -->
        @Scripts.Render("~/bundles/bootstrap")
        <!-- iCheck -->
        @Scripts.Render("~/bundles/iCheck")
        <!-- select2 -->
        @Scripts.Render("~/bundles/select2")
        <!-- AdminLTE App -->
        @Scripts.Render("~/bundles/theme")
        <!-- Angular -->
        @Scripts.Render("~/bundles/angular")
        <!-- DataTable -->
        @*@Scripts.Render("~/bundles/datatable")*@
        <script type="text/javascript" src="~/Areas/SuperAdmin/Scripts/Controllers/SideMenu.js"></script>
        @RenderSection("scripts", required: false)

        <!-- Page script -->
        <script>
            $(function () {
                //Initialize Select2 Elements
                //$('.select2').select2();

            })
        </script>

    </body>

</html>